            android:name="com.example.ahack.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        <activity
            android:name=".Welcome"></activity>
        <activity
            android:name=".Toss"></activity>
        <activity
            android:name=".Go"></activity>
        <activity
            android:name=".Anim"></activity>
        <activity
            android:name=".Wait"></activity>
        <activity
            android:name=".Story"></activity>
